<div>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    {{#each users.data}}
        {{> ../partials/userTableTr this}}
    {{/each}}
  </tbody>

</table>
<div class="row justify-content-md-center">
    <nav aria-label="Page navigation example mx-auto">
    <ul class="pagination">
        {{#pagesTimesHlp users.total_pages}}
        <li class="page-item {{#equalHlp ../selected this}}active{{/equalHlp}}">
            <a class="page-link" href="{{this}}">{{this}}
                {{#equalHlp ../selected this}}
                <span class="sr-only">(current)</span>
                {{/equalHlp}}
            </a>
        </li>
        {{/pagesTimesHlp}}
    </ul>
    </nav>
</div>
</div>